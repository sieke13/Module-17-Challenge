{
"compilerOptions": {
  "target": "ES2020",
  "module":"ESNext",
  "moduleResolution": "bundler",
  "allowJs": true,
  "rootDir": "./src",
  "outDir": "./dist",
  "strict": true,
  "checkJs": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true
},
"include": ["src"]
}